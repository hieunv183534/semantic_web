@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix : <http://example.org/schemas/live_concert#> .


#===== Defnitions ================

#===== Classes ========

:ConcertTour a owl:Class .
:LiveConcert a owl:Class .
:Artist a owl:Class .
:Song a owl:Class .
:Repertoire rdfs:subClassOf [ 
    a owl:Restriction ; 
    owl:minQualifiedCardinality "2"^^xsd:nonNegativeInteger ; 
    owl:Property :repertoireSongs ;
    owl:onClass :Song
] .

:Location a owl:Class .

:Genre a owl:Class .
:Rock rdfs:subClassOf :Genre .
:Electronica rdfs:subClassOf :Genre .
:Doom rdfs:subClassOf :Rock .
:HipHop rdfs:subClassOf :Genre .
:Metal rdfs:subClassOf :Rock .

#======================

#===== Properties =====

:concerts a owl:ObjectProperty ;
	rdfs:domain :ConcertTour ;
	rdfs:range :LiveConcert .

:concertArtist a owl:ObjectProperty ;
	rdfs:domain :LiveConcert ;
	rdfs:range :Artist .

:concertDate a rdf:DataTypeProperty ;
	rdfs:domain :LiveConcert ;
	rdfs:range xsd:dateTime .

:concertLocation a owl:ObjectProperty ;
	rdfs:domain :LiveConcert ;
	rdfs:range :Location .

:location a rdfs:DataTypeProperty ;
	rdfs:domain :Location ;
	rdfs:range rdfs:Literal .

:artistName a owl:ObjectProperty ;
	rdfs:domain :Artist ;
    rdfs:range xsd:String .

:artistGenre a owl:ObjectProperty ;
	rdfs:domain :Artist ;
	rdfs:range :Genre .

:artistRepertoire a owl:ObjectProperty ;
	rdfs:domain :Artist ;
	rdfs:range :Repertoire .

:repertoireSongs a owl:ObjectProperty ;
	rdfs:domain :Repertoire ;
	rdfs:range :Song .

:songName a owl:DataTypeProperty ;
	rdfs:domain :Song ;
	rdfs:range xsd:String .

